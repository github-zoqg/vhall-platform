NODE_ENV=production

# 接口域名
VUE_APP_BASE_URL=https://saas-api.vhall.com

# 接口mock拼接字段
VUE_APP_MOCK_BASE_URL=/mock

# 公共资源域名
VUE_APP_PUBLIC_PATH=//cnstatic01.e.vhall.com

# 路由base
VUE_APP_ROUTER_BASE_URL=/v3

# 观看端域名
VUE_APP_WAP_WATCH=//live.vhall.com

# 发起端+控制台域名
VUE_APP_WEB_BASE=//e.vhall.com

# 发起端+控制台的二级标记
VUE_APP_WEB_KEY=/v3