{
  "name": "middle-platform",
  "version": "7.9.30",
  "domainVersion": "1.5.11",
  "private": true,
  "scripts": {
    "/---/": "-------------saas业务线相关命令----------------",
    "serve": "npm run serve:saas-live:test",
    "/-----test-----/": "------------基于 test 环境开发启动命令------------------",
    "serve:saas-live:test": "node scripts/cli-service.js serve --project=saas-live --mode=dev_test",
    "serve:saas-wap:test": "node scripts/cli-service.js serve --project=saas-wap --mode=dev_test",
    "serve:saas-watch:test": "node scripts/cli-service.js serve --project=saas-watch --mode=dev_test",
    "/-----test4-----/": "------------基于 test4 环境开发启动命令------------------",
    "serve:saas-live:test4": "node scripts/cli-service.js serve --project=saas-live --mode=dev_test4",
    "serve:saas-wap:test4": "node scripts/cli-service.js serve --project=saas-wap --mode=dev_test4",
    "serve:saas-watch:test4": "node scripts/cli-service.js serve --project=saas-watch --mode=dev_test4",
    "/-----new_test----/": "------------基于 new_test 环境开发启动命令--------------",
    "serve:saas-live:new_test": "node scripts/cli-service.js serve --project=saas-live --mode=dev_new_test",
    "serve:saas-wap:new_test": "node scripts/cli-service.js serve --project=saas-wap --mode=dev_new_test",
    "serve:saas-watch:new_test": "node scripts/cli-service.js serve --project=saas-watch --mode=dev_new_test",
    "/-----test_zt----/": "-------------基于 test_zt 环境开发启动命令---------------",
    "serve:saas-live:test_zt": "node scripts/cli-service.js serve --project=saas-live --mode=dev_test_zt",
    "serve:saas-wap:test_zt": "node scripts/cli-service.js serve --project=saas-wap --mode=dev_test_zt",
    "serve:saas-watch:test_zt": "node scripts/cli-service.js serve --project=saas-watch --mode=dev_test_zt",
    "/-----pre----/": "-------------预发环境构建命令---------------",
    "build:saas-live:pre": "node scripts/cli-service.js build --project=saas-live --mode=pre",
    "build:saas-wap:pre": "node scripts/cli-service.js build --project=saas-wap --mode=pre",
    "build:saas-watch:pre": "node scripts/cli-service.js build --project=saas-watch --mode=pre",
    "/***/": "-------------其它命令----------------",
    "update:version": "node ./scripts/update-version.js serve",
    "help:lint": "node_modules/.bin/vue-cli-service help lint",
    "format": "prettier --write './src/**/*.(vue|js|css|less|jsx)' './scripts/**/*.js'",
    "lint": "node_modules/.bin/vue-cli-service lint --no-fix",
    "lint:fix": "node_modules/.bin/vue-cli-service lint"
  },
  "dependencies": {
    "better-scroll": "^2.4.2",
    "clipboard": "^2.0.8",
    "core-js": "^3.6.5",
    "css-vars-ponyfill": "^2.4.7",
    "element-resize-detector": "^1.2.4",
    "element-ui": "^2.15.6",
    "highlight.js": "^11.3.1",
    "overlayscrollbars-vue": "^0.2.2",
    "postcss-pxtorem": "5",
    "qrcode": "^1.5.0",
    "screenfull": "^5.2.0",
    "semver": "^6.3.0",
    "svgaplayerweb": "^2.3.2",
    "ua-parser-js": "^0.7.22",
    "vue": "^2.6.14",
    "vue-highlightjs": "^1.3.3",
    "vue-i18n": "^8.26.7",
    "vue-router": "^3.5.2",
    "vue-virtual-scroll-list": "^2.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "autoprefixer": "^8.6.5",
    "axios": "^0.27.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "chalk": "4.1.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "filemanager-webpack-plugin": "^6.1.7",
    "fs-extra": "^10.0.0",
    "fs-walk": "^0.0.2",
    "less": "3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "lodash-webpack-plugin": "^0.11.6",
    "n-readlines": "^1.0.1",
    "ora": "4.1.1",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.2.1",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "style-resources-loader": "^1.5.0",
    "vue-awesome-swiper": "^3.1.3",
    "vue-cli-plugin-style-resources-loader": "^0.1.5",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "node_modules/.bin/prettier --write",
      "node_modules/.bin/vue-cli-service lint"
    ]
  }
}