{
  "name": "middle-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "/---/": "-------------saas业务线相关命令----------------",
    "serve": "npm run serve:saas-live",
    "serve:doc": "node scripts/cli-service.js serve --project=doc",
    "serve:saas-live": "node scripts/cli-service.js serve --project=saas-live",
    "serve:saas-wap": "node scripts/cli-service.js serve --project=saas-wap",
    "serve:saas-watch": "node scripts/cli-service.js serve --project=saas-watch",
    "serve:zhike-live": "node scripts/cli-service.js serve --project=zhike-live",
    "build:saas-live": "node scripts/cli-service.js build --project=saas-live",
    "build:saas-live:test": "node scripts/cli-service.js build --project=saas-live --mode=test",
    "build:saas-wap": "node scripts/cli-service.js build --project=saas-wap",
    "build:saas-watch": "node scripts/cli-service.js build --project=saas-watch",
    "/***/": "-------------其它命令----------------",
    "help:lint": "node_modules/.bin/vue-cli-service help lint",
    "format": "prettier --write './src/**/*.(vue|js|css|less|jsx)' './scripts/**/*.js'",
    "lint": "node_modules/.bin/vue-cli-service lint --no-fix",
    "lint:fix": "node_modules/.bin/vue-cli-service lint"
  },
  "dependencies": {
    "@sentry/tracing": "^6.16.1",
    "@sentry/vue": "^6.16.1",
    "clipboard": "^2.0.8",
    "core-js": "^3.6.5",
    "element-resize-detector": "^1.2.4",
    "element-ui": "^2.15.6",
    "highlight.js": "^11.3.1",
    "overlayscrollbars": "^1.13.1",
    "overlayscrollbars-vue": "^0.2.2",
    "postcss-pxtorem": "5",
    "qrcode": "^1.5.0",
    "screenfull": "^5.2.0",
    "vue": "^2.6.14",
    "semver": "^6.3.0",
    "vue-highlightjs": "^1.3.3",
    "vant": "^2.1.5",
    "ua-parser-js": "^0.7.22",
    "vue-i18n": "^8.26.7",
    "vue-router": "^3.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@sentry/webpack-plugin": "^1.18.4",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "autoprefixer": "^8.6.5",
    "babel-eslint": "^10.1.0",
    "better-scroll": "^2.4.2",
    "chalk": "4.1.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "filemanager-webpack-plugin": "^6.1.7",
    "fs-extra": "^10.0.0",
    "fs-walk": "^0.0.2",
    "less": "3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "ora": "4.1.1",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.2.1",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "style-resources-loader": "^1.5.0",
    "vue-awesome-swiper": "^3.1.3",
    "vue-cli-plugin-style-resources-loader": "^0.1.5",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "node_modules/.bin/prettier --write",
      "node_modules/.bin/vue-cli-service lint"
    ]
  }
}
